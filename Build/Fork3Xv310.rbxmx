			local m33=-m15-m16+m17-m12;if m33*m33&lt;m08 then local m34=-m18-m19+m20-m13;if m34*m34&lt;m11 then local m35=-m21-m22+m23-m14;if m35*m35&lt;m05 then return true;end;end;end;
