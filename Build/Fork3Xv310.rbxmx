        {
			CloseButton = new(ImageButton, {
				Image = 'rbxassetid://2244452978',
				ImageRectOffset = Vector2.new(0, 0),
				ImageRectSize = Vector2.new(14, 14) * 2,
				AspectRatio = 1,
				AnchorPoint = Vector2.new(1, 0.5),
