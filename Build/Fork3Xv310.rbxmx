function ShowUI()
	
	UI = Tool:WaitForChild('UI')
	ColorPicker = require(UI:WaitForChild('ColorPicker'))
	-- Creates and reveals the UI

	-- Reveal UI if already created
	if PaintTool.UI and PaintTool.UI.Parent ~= nil then

		-- Reveal the UI
