			[Roact.Event.MouseEnter] = function (rbx)
				rbx.BackgroundTransparency = 0.2
			end;
			[Roact.Event.InputEnded] = function (rbx)
				rbx.BackgroundTransparency = (self.props.CurrentOption == Option) and 0.1 or 1
			end;
